{
  "name": "bsdlp",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://bsdlp.com",
  "dependencies": {
    "eslint-plugin-import": "^2.18.2",
    "gh-pages": "^1.2.0",
    "npm-css": "^0.2.3",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "5.0.1",
    "tachyons": "^4.11.1",
