    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^3.1.0",
    "style-loader": "^0.18.2",
    "tachyons-font-family": "^4.3.4"
  }
